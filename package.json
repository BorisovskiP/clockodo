{
    "name": "clockodo",
    "version": "1.0.0",
    "description": "The Node/JavaScript Client for the Clockodo-API",
    "main": "./lib/api.js",
    "babel": {
        "presets": [
            [
                "env",
                {
                    "targets": {
                        "node": "6"
                    }
                }
            ]
        ]
    },
    "scripts": {
        "build": "babel src -d lib",
        "lint": "eslint lib tests",
        "pretest": "npm run build",
        "test": "jest",
        "posttest": "npm run lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/peerigon/clockodo-node.git"
    },
    "keywords": ["node", "javascript", "clockodo", "work time", "project report", "time tracking", "api"],
    "author": "developers@peerigon.com",
    "license": "Unlicense",
    "bugs": {
        "url": "https://github.com/peerigon/clockodo-node/issues"
    },
    "homepage": "https://github.com/peerigon/clockodo-node#readme",
    "dependencies": {
        "axios": "^0.18.0",
        "babel-cli": "^6.26.0",
        "camelcase": "^5.0.0",
        "deep-map-keys": "^1.2.0",
        "moment": "^2.21.0",
        "qs": "^6.5.1"
    },
    "devDependencies": {
        "babel-preset-env": "^1.6.1",
        "dotenv": "^5.0.1",
        "eslint": "^4.18.2",
        "eslint-config-peerigon": "^12.0.1",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-jsdoc": "^3.5.0",
        "jest": "^22.4.2",
        "nock": "^9.2.3"
    },
    "jest": {
        "collectCoverage": true,
        "testEnvironment": "node"
    }
}
