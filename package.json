{
  "name": "clockodo",
  "version": "4.0.0",
  "description": "Unofficial Node.js SDK for the Clockodo API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "dev": "npm run build -- -w",
    "lint": "eslint src tests --ext js,ts",
    "test": "jest",
    "test:unit": "jest tests/unit/unit.test.ts",
    "test:types": "tsc --noEmit",
    "posttest": "run-p lint test:types",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerigon/clockodo.git"
  },
  "keywords": [
    "time tracking",
    "api",
    "sdk"
  ],
  "author": "developers@peerigon.com",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/peerigon/clockodo/issues"
  },
  "homepage": "https://github.com/peerigon/clockodo#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "axios-cache-adapter": "^2.5.0",
    "camelcase-keys": "^6.2.2",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.2",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-peerigon": "^25.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "jest": "^26.4.2",
    "nock": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "dist",
    "plugins"
  ],
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
